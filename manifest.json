{
    "manifest_version": 2,
    "name": "skip-ad-youtube",
    "version": "1.0",
    "description": "skips ads that appear on youtube.",

    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],
            "js": ["content.js"],
            "css": ["style.css"]
        }
    ]


}